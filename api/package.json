    "global": "^4.4.0",
    "method-override": "^2.3.x",
    "moment": "^2.23.0",
    "multer": "^1.4.1",
    "mysql": "^2.18.1",
    "mysql2": "^1.6.4",
    "nconf": "^0.6.x",
    "nodemon": "^1.12.1",
